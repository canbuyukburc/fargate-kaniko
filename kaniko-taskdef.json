{
    "family": "kaniko-jenkins",
    "taskRoleArn": "arn:aws:iam::111222333444:role/Kaniko_Task_Role",
    "executionRoleArn": "arn:aws:iam::111222333444:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "jenkins-agent",
            "image": "111222333444.dkr.ecr.eu-west-1.amazonaws.com/jenkins:inbound-agent",
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "kaniko-builder",
                    "awslogs-region": "eu-west-1",
                    "awslogs-stream-prefix": "jenkins-agent"
                }
            },
            "essential": true,
            "mountPoints": [
                {
                    "sourceVolume": "sourcecode",
                    "containerPath": "/data",
                    "readOnly": false
                }
            ]
        },
        {
            "name": "kaniko",
            "image": "111222333444.dkr.ecr.eu-west-1.amazonaws.com/kaniko-debug",
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "kaniko-builder",
                    "awslogs-region": "eu-west-1",
                    "awslogs-stream-prefix": "kaniko"
                }
            },
            "essential": false,
            "mountPoints": [
                {
                    "sourceVolume": "sourcecode",
                    "containerPath": "/data",
                    "readOnly": false
                }
            ],
            "entryPoint": ["/busybox/sh"],
            "command": ["/docker-entrypoint.sh"]
        }],
        "volumes": [
            {
                "name": "sourcecode",
                "host": {}
            }
        ],
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "512",
    "memory": "1024"
}